{
  "defaultSeverity": "warning",
  "rules": {
    "no-unused-variable":true,
    "variable-name": false,
    "no-empty": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-console": [
      true,
      "warn",
      "error"
    ],
    "no-shadowed-variable": false
  },
  "extends": "tslint:recommended",
  "rulesDirectory": [
      "node_modules/codelyzer"
  ]

}
